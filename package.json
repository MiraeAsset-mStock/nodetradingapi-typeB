{
  "name": "mconnectb",
  "version": "0.0.0",
  "description": "The official TypeScript/JavaScript client for MConnect TypeB Trading API",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "module": "dist/lib/index.js",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "test": "ts-node test/test-api.ts",
    "test-ticker": "ts-node test/test-ticker.ts",
    "ticker-example":"ts-node example/example-ticker.ts"
  },
  "keywords": [
    "mconnect",
    "typeb",
    "trading",
    "api",
    "typescript",
    "javascript",
    "websocket",
    "market-data",
    "orders",
    "stock-trading",
    "financial-api",
    "real-time-data"
  ],
  "author": {
    "name": "MConnect Team",
    "email": "support@mconnect.com"
  },
  "license": "MIT",
  "homepage": "https://www.npmjs.com/package/mconnecttypea",
  "bugs": {
    "url": "https://www.npmjs.com/package/mconnecttypea"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "mconnecttypea": "^1.0.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.10",
    "@types/ws": "^8.5.0",
    "cross-env": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://www.npmjs.com/package/mconnecttypea"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
