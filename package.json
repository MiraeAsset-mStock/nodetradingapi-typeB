{
  "name": "@mstock/mconnectb",
  "version": "1.0.0",
  "description": "The official TypeScript/JavaScript client for mConnect TypeB Trading API",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "module": "dist/lib/index.js",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "test": "ts-node test/test.ts",
    "ticker": "ts-node test/test-websocket.ts",
    "example-test": "ts-node example/websocket-stream.ts"
  },
  "keywords": [
    "mconnect",
    "typeb",
    "trading",
    "api",
    "typescript",
    "javascript",
    "websocket",
    "market-data",
    "mstock",
    "stock-market",
    "financial-api",
    "real-time-data"
  ],
  "author": {
    "name": "mStock by Mirae Asset"
  },
  "license": "MIT",
  "homepage": "https://github.com/MiraeAsset-mStock/nodetradingapi-typeB#readme",
  "bugs": {
    "url": "https://github.com/MiraeAsset-mStock/nodetradingapi-typeB/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@mstock-mirae-asset/nodetradingapi-typeb": "^0.0.2",
    "axios": "^1.6.0",
    "node": "^24.8.0",
    "tsc": "^2.0.4",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.10",
    "@types/ws": "^8.5.0",
    "cross-env": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiraeAsset-mStock/nodetradingapi-typeB.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "constants": "constants",
    "types": "types",
    "test": "test"
  }
}
